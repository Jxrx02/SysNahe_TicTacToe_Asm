                       447     DISPLAY_BOARD:
                       448             ;-----------------------------------------------------------------------
                       449                 ; Zeige R7,R6,R5 nacheinander gemultiplext an
                       450                 ; R7, R6, R5 spiegeln das Feld wider, x die Eingabe aus R0 von Funktion ON_INPUT (Wird au
                       451             ;-----------------------------------------------------------------------
01F2 759000            452             MOV P1, #00000000b ;setze p1 standardgemäß auf 0
                       453             ;display0:
01F5 8FA0              454                     MOV P2, R7
01F7 D290              455                     setb P1.0
01F9 109000            456                     JBC P1.0,display1
                       457             display1:
                       458                     ;MOV P2, R7
01FC D291              459                     setb P1.1
01FE 109100            460                     JBC P1.1,display2
                       461             display2:
                       462                     ;MOV P2, #00000000b
0201 D292              463                     setb P1.2
0203 109200            464                     JBC P1.2,display3
