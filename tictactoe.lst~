tictactoe                                                                                                               PAGE 1
                         1     ;-------------------------------------------------
                         2     ;TicTacToe, 2 Spieler
                         3     ; Eingabe: Matrix-Keypad an P0 (P0.0-P0.3, für 9 Felder), Eingabe bestätigen P3.2; P3.2 muss direkt w
                         4     ; Ausgabe: 8x8-LED-Matrix an P1, P2
                         5     ;
                         6     ; -------------------------------------------------
                         7
                         8
                         9     ORG 0000H   ; Startadresse des Programms
0000 8013               10     LJMP INIT_BOARD
                        11
                        12     ;-----------------------------------------------------------------------
                        13     ;ISR
                        14     ;-----------------------------------------------------------------------
                        15     ORG 0003H ; Einsprungsadresse von Interrupt 3.2
0003 802C               16     LJMP ON_INPUT
                        17
                        18     ORG 0013H ; Einsprungsadresse von Interrupt 3.3
0013 01BD               19     LJMP CLEAR_FIELD
                        20
                        21     ;-----------------------------------------------------------------------
                        22     ; Logik
                        23     ;-----------------------------------------------------------------------
                        24     INIT_BOARD:
                        25             ;Bits für Interrupt
                        26             ;SETB IT0       ; Externer Interrupt reagiert auf fallende Flanke an P3.2; -> ist für richtig
0016 C288               27             CLR IT0         ; Externer Interrupt reagiert auf gedrückten Schalter P3.2
0018 D2A8               28             SETB EX0        ; Externen Interrupt aktivieren
001A C28A               29             CLR IT1         ; Externer Interrupt reagiert auf gedrückten Schalter P3.3
001C D2AA               30             SETB EX1        ; Externen Interrupt aktivieren
001E D2AF               31             SETB EA         ; Interrupts generell zulassen
                        32              ; --- ab hier reagiert der µC auf den externen Interrupt 0 und springt auf Adresse 03H (ISR)
0020 7F00               33             mov r7, #00000000b      ;00 | 00 | 01
                        34                                     ;00 | 00 | 01           wenn x < 3 -> R7
                        35                                     ;-- - -- - --
0022 7E00               36             mov r6, #00000000b      ;11 | 01 | 00
                        37                                     ;11 | 01 | 00           wenn x < 6 -> R6
                        38                                     ;-- | -- | --
0024 7D00               39             mov r5, #00000000b      ;11 | 00 | 00
                        40                                     ;11 | 00 | 00           sonst R5
                        41
0026 759000             42             MOV P1, #00H    ; Reset Register Port 1 als Ausgangsport für LED-Matrix; Befehl nicht notwend
0029 75A000             43             MOV P2, #00H    ; Reset Register Port 2 als Ausgangsport für LED-Matrix; Befehl nicht notwend
                        44
002C 8000               45             LJMP MAIN_LOOP
                        46
                        47
                        48     MAIN_LOOP:
                        49             ;-----------------------------------------------------------------------
                        50                 ; ON_INPUT: Wird P3.2 betätigt, so wird Port P0 (P0.0-P0.3) eingelesen -> liefert Dezimal
                        51             ;-----------------------------------------------------------------------
002F 31E9               52             LCALL DISPLAY_BOARD
0032 80FC               53             JMP MAIN_LOOP
                        54
                        55
                        56     ON_INPUT:
0034 E5B0               57             MOV a, P3
                        58
0036 540F               59             ANL a, #00FH ; bitweise &-Verknüpfung, um nur die letzten 4 Bits (P0.0-P0.3) auszuwerten
0038 F8                 60             MOV R0, a
                        61             ;-----------------------------------------------------------------------
                        62                 ; Mappe Feld auf 8x8 Matrix
                        63                 ; => R0 enthält den Wert des zu setztenden Feldes
                        64             ;-----------------------------------------------------------------------
                        65             ; x < 3
0039 9403               66             SUBB A, #03h; Idee: Angenommen: Eingabe 8  soll auf Feld 8 (mitte unten) gemappt werden -> sc
                        67                             ;bestimme die Mitte durch Bitshifts
                        68                             ;
                        69                             ; 8-3 >0; 5-3>0; 2-3 = -1 (^=n) Setze Carry oder wenn akku leer, springe auf
003B 400E               70             JC LESS_THAN_3 ; x<3 -> schreibe in R7
003D 600C               71             JZ LESS_THAN_3
                        72
                        73             ; 3 < x < 6
003F 9403               74             SUBB A, #03h
0041 4027               75             JC LESS_THAN_6 ; x<6 -> schreibe in R6
0043 6025               76             JZ LESS_THAN_6
                        77
                        78             ; 6< x < 9
0045 9403               79             SUBB A, #03h
0047 4040               80             JC LESS_THAN_9 ; x<9 -> schreibe in R5
0049 603E               81             JZ LESS_THAN_9
                        82
                        83             ; x < 3
                        84             LESS_THAN_3:
004B FA                 85                     MOV R2, A; register für zähler der foor loop
                        86                     ; wenn Bit P3.7 gesetzt ist, so schreibe #00000001b (Spieler 2), ansonsten #00000011b
004C 20B75D             87                     JB P3.7, WRITE_PLAYER_2_INTO_FIELD
004F 30B756             88                     JNB P3.7, WRITE_PLAYER_1_INTO_FIELD
                        89                     NACH_REGISTER_FÜLLEN1:
0052 B2B7               90                             CPL P3.7
0054 CA                 91                             xch A,R2                ; dopppelte Vertauschung notwendig!!
0055 C2D6               92                             CLR AC
0057 10D706             93                             JBC CY,SUM_LOOP_3
                        94
                        95                     JoaNhBinZuUnkreativ1:
005A CA                 96                             xch A,R2                ;tausche A mit R2
005B 6F                 97                             XRL A, R7               ;schreibe wert in TicTacToeFeld mit exclusiv-oder-Ver
005C FF                 98                             MOV R7, A               ;speichere Wert in Register zwischen
005D 11CE               99                             LCALL CHECK_FOR_WIN
0060 32                100                             RETI                    ;return from interupt; remove interrupt-bit
                       101
                       102             SUM_LOOP_3:
0061 CA                103                     xch A,R2
0062 0A                104                     INC R2                          ;Inkrementiere den Zählerwert für die nächste Iterati
0063 23                105                     RL A                            ;rotiere akku um 3 Stellen je übertrag
0064 23                106                     RL A
0065 23                107                     RL A
0066 CA                108                     xch A,R2                        ;tausche A(kku) mit R2
0067 70F8              109                     JNZ SUM_LOOP_3                  ;springe zurück zur Schleife, wenn r2 != 0
0069 60F0              110                     JZ JoaNhBinZuUnkreativ1         ;sonst speichere Wert in R7
                       111
                       112             ;-----------------------------------------------------------------------
                       113             ; 3 < x < 6
                       114             LESS_THAN_6:
006B FA                115                     MOV R2, A
006C 20B746            116                     JB P3.7, WRITE_PLAYER_2_INTO_FIELD2
006F 30B73F            117                     JNB P3.7, WRITE_PLAYER_1_INTO_FIELD2
                       118                     NACH_REGISTER_FÜLLEN2:
0072 B2B7              119                             CPL P3.7
0074 CA                120                             xch A,R2
0075 C2D6              121                             CLR AC
0077 10D706            122                             JBC CY,SUM_LOOP_6
                       123
                       124                     JoaNhBinZuUnkreativ2:
007A CA                125                             xch A,R2
007B 6E                126                             XRL A, R6
007C FE                127                             MOV R6, A
007D 11CE              128                             LCALL CHECK_FOR_WIN
0080 32                129                             RETI
                       130
                       131             SUM_LOOP_6:
0081 CA                132                     xch A,R2
0082 0A                133                     INC R2
0083 23                134                     RL A
0084 23                135                     RL A
0085 23                136                     RL A
0086 CA                137                     xch A,R2
0087 70F8              138                     JNZ SUM_LOOP_6
0089 60F0              139                     JZ JoaNhBinZuUnkreativ2
                       140
                       141
                       142
                       143             ;-----------------------------------------------------------------------
                       144             ; 6< x < 9
                       145             LESS_THAN_9:
008B FA                146                     MOV R2, A
008C 20B72F            147                     JB P3.7, WRITE_PLAYER_2_INTO_FIELD3
008F 30B728            148                     JNB P3.7, WRITE_PLAYER_1_INTO_FIELD3
                       149                     NACH_REGISTER_FÜLLEN3:
0092 B2B7              150                             CPL P3.7
0094 CA                151                             xch A,R2
0095 C2D6              152                             CLR AC
0097 10D706            153                             JBC CY,SUM_LOOP_9
                       154
                       155                     JoaNhBinZuUnkreativ3:
009A CA                156                             xch A,R2
009B 6D                157                             XRL A, R5
009C FD                158                             MOV R5, A
009D 11CE              159                             LCALL CHECK_FOR_WIN
00A0 32                160                             RETI
                       161
                       162             SUM_LOOP_9:
00A1 CA                163                     xch A,R2
00A2 0A                164                     INC R2
00A3 23                165                     RL A
00A4 23                166                     RL A
00A5 23                167                     RL A
00A6 CA                168                     xch A,R2
00A7 70F8              169                     JNZ SUM_LOOP_9
00A9 60F0              170                     JZ JoaNhBinZuUnkreativ3
                       171
                       172     ; Das hier ist bisschen unschön, aber es muss zu der jeweiligen Adresse im 'IF' zurückgesprungen werd
                       173     WRITE_PLAYER_1_INTO_FIELD:
00AB 7403              174             MOV A, #00000011b
00AD 80A6              175             JMP NACH_REGISTER_FÜLLEN1
                       176     WRITE_PLAYER_2_INTO_FIELD:
00AF 7401              177             MOV A, #00000001b
00B1 80A2              178             JMP NACH_REGISTER_FÜLLEN1
                       179
                       180     WRITE_PLAYER_1_INTO_FIELD2:
00B3 7403              181             MOV A, #00000011b
00B5 80BD              182             JMP NACH_REGISTER_FÜLLEN2
                       183     WRITE_PLAYER_2_INTO_FIELD2:
00B7 7401              184             MOV A, #00000001b
00B9 80B9              185             JMP NACH_REGISTER_FÜLLEN2
                       186
                       187     WRITE_PLAYER_1_INTO_FIELD3:
00BB 7403              188             MOV A, #00000011b
00BD 80D4              189             JMP NACH_REGISTER_FÜLLEN3
                       190     WRITE_PLAYER_2_INTO_FIELD3:
00BF 7401              191             MOV A, #00000001b
00C1 80D0              192             JMP NACH_REGISTER_FÜLLEN3
                       193
                       194
                       195     CLEAR_FIELD:
00C3 7F00              196             MOV R7, #00H
00C5 7E00              197             MOV R6, #00H
00C7 7D00              198             MOV R5, #00H
00C9 7C00              199             MOV R4, #00H
00CB 7B00              200             MOV R3, #00H
00CD 7A00              201             MOV R2, #00H
00CF 7900              202             MOV R1, #00H
00D1 7800              203             MOV R0, #00H
00D3 32                204             RETI
                       205
                       206
                       207     CHECK_FOR_WIN:
00D4 8000              208             LJMP CHECK_HORIZONTAL
                       209             CHECK_HORIZONTAL:
                       210             ;Waagerechte
                       211             ;a: Für Spieler 2 (Ganzes Feld) für jedes Register (5-7), |-Verknüpfung mit 00100100, addiere
                       212             ;b: Für Spieler 1 vorher noch #10101010b mit or-verknüpfung, dann a: mit anderen zählerstand
                       213                     ;Spieler mit ganzen Feldern
                       214                     ;MOV R7, #01001001b     ;zum Testen
00D7 EF                215                     MOV A, R7
00D8 4424              216                     ORL A, #00100100b
00DA 04                217                     INC A
00DB 6020              218                     JZ INC_COUNTER_1
                       219
00DD EE                220                     MOV A, R6
00DE 4424              221                     ORL A, #00100100b
00E0 04                222                     INC A
00E1 601A              223                     JZ INC_COUNTER_1
                       224
00E3 ED                225                     MOV A, R5
00E4 4424              226                     ORL A, #00100100b
00E6 04                227                     INC A
00E7 6014              228                     JZ INC_COUNTER_1
                       229
                       230                     ;Spieler mit halben Feldern
00E9 EF                231                     MOV A, R7                       ; exclusiv oder, da das vollständig gefüllte Feld die
00EA 64B6              232                     XRL A, #10110110b
00EC 04                233                     INC A
00ED 6010              234                     JZ INC_COUNTER_2
                       235
00EF EE                236                     MOV A, R6
00F0 64B6              237                     XRL A, #10110110b
00F2 04                238                     INC A
00F3 600A              239                     JZ INC_COUNTER_2
                       240
00F5 ED                241                     MOV A, R5
00F6 64B6              242                     XRL A, #10110110b
00F8 04                243                     INC A
00F9 6004              244                     JZ INC_COUNTER_2
                       245
00FB 800E              246                     LJMP CHECK_VERTICAL
                       247
                       248             ;-----------------------------------------------------------------------
                       249             ; JA DIE METHODENDEKLARATION IST HIER SUPER VERWIRREND.
                       250             ; habe das hier hingepasted, da sonst die Adressen nicht gefunden werden, weil anscheinend di
                       251             ; Wenn die Deklaration und der tatsächliche Aufruf zu weit auseinander stehen, dann findet er
                       252             ;-----------------------------------------------------------------------
                       253             INC_COUNTER_1:          ; R4 beinhaltet Punktestand von S1 und S2: 0000 | 0000
00FE 0C                254                     INC R4
00FF 22                255                     RET ;JMP MAIN_LOOP
                       256             INC_COUNTER_2:
0100 CC                257                     XCH A, R4
0101 23                258                     RL A
0102 23                259                     RL A
0103 23                260                     RL A
0104 23                261                     RL A
0105 04                262                     INC A
0106 23                263                     RL A
0107 23                264                     RL A
0108 23                265                     RL A
0109 23                266                     RL A
010A CC                267                     XCH A, R4
010B 22                268                     RET
                       269
                       270             CHECK_VERTICAL:
                       271             ;Senkrechte
                       272             ;a: für Register R7, R6, R5 jeweils &-Verknüpfung #11X00000b;
                       273             ;b: rotiere alu 2x
                       274             ;c: fülle auf und addiere -> Carry -> Zählerstand
                       275             ;linke reihe
010C EF                276                     MOV A, R7
010D 75F000            277                     MOV B, #00h
0110 54C0              278                     ANL A, #11000000b       ; nur linke Reihe
0112 23                279                     RL A                    ; rotiere Alu 2x
0113 23                280                     RL A
0114 C5F0              281                     XCH A,B
                       282
0116 EE                283                     MOV A, R6
0117 54C0              284                     ANL A, #11000000b       ; nur linke Reihe
0119 45F0              285                     ORL A, B
011B 23                286                     RL A
011C 23                287                     RL A
011D 75F000            288                     MOV B, #00h     ;evtl nicht notwendig
0120 C5F0              289                     XCH A,B
                       290
0122 ED                291                     MOV A, R5
0123 54C0              292                     ANL A, #11000000b       ; nur mittlere Reihe
0125 45F0              293                     ORL A, B                ; speichere Wert in B zwischen
0127 F5F0              294                     MOV B, A
                       295
0129 6418              296                     XRL A, #0011000b        ; fülle
012B 04                297                     INC A                   ; Zählerstand
012C 60D0              298                     JZ INC_COUNTER_1
                       299
012E C5F0              300                     XCH A,B
0130 64BA              301                     XRL A, #10111010b       ; fülle                         HOFFENTLICH STIMMT DIE BITMAP
0132 04                302                     INC A                   ; Zählerstand
0133 75F000            303                     MOV B, #00h             ; leere B
0136 60C8              304                     JZ INC_COUNTER_2
                       305
                       306             ; mittlere reihe
0138 EF                307                     MOV A, R7
