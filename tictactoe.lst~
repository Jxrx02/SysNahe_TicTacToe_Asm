                        38
                        39
                        40     INIT_BOARD:
                        41             mov r7, #00000000b      ;00 0 00 0 10
                        42                                     ;00 0 00 0 10           wenn x < 3 -> R7
                        43                                     ;00 0 00 0 00
                        44             mov r6, #00000000b      ;11 0 10 0 00
                        45                                     ;11 0 10 0 00           wenn x < 6 -> R6
                        46                                     ;00 0 00 0 00
                        47             mov r5, #00000000b      ;11 0 00 0 00
                        48                                     ;11 0 00 0 00           sonst R5
                        49
                        50
                        51
                        52     MAIN_LOOP:
                        53             ;-----------------------------------------------------------------------
                        54                 ; ON_INPUT: Wird P0.7 betätigt, so wird Port P0 (P0.0-P0.3) eingelesen -> liefert Dezimal
                        55             ;-----------------------------------------------------------------------
                        56             JB P0.7, ON_INPUT
                        57
                        58             call DISPLAY_BOARD
                        59             JMP MAIN_LOOP
                        60
                        61
                        62     ON_INPUT:
                        63             MOV a, P0
                        64             ANL a, #00FH ; bitweise &-Verknüpfung, um nur die letzten 4 Bits (P0.0-P0.3) auszuwerten
                        65             MOV R0, a
                        66             ;-----------------------------------------------------------------------
                        67                 ; Mappe Feld auf 8x8 Matrix
                        68                 ; => R0 enthält den Wert des zu setztenden Feldes
                        69             ;-----------------------------------------------------------------------
                        70             ; x < 3
                        71             SUBB A, #03h; Idee: 8-3 >0; 5-3>0; 2-3 = -1 Setze Carry oder wenn akku leer, springe auf schl
                        72             JC LESS_THAN_3 ; x<3 -> schreibe in R7
                        73             JZ LESS_THAN_3
                        74
                        75             ; 3 < x < 6
                        76             SUBB A, #03h
                        77             JC LESS_THAN_6 ; x<6 -> schreibe in R6
                        78             JZ LESS_THAN_6
                        79             ; 6< x < 9
                        80             SUBB A, #03h
                        81             JC LESS_THAN_9 ; x<6 -> schreibe in R6
                        82             JZ LESS_THAN_9
                        83
                        84             ; x < 3
                        85             LESS_THAN_3:
                        86                     MOV R2, A; register für zähler der foor loop
                        87                     ; wenn Bit für S1 gesetzt ist, so schreibe #00000011b, ansonsten #00000001b
                        88                     MOV A, #00000011b
                        89                     xch A,R2                ; dopppelte Vertauschung notwendig!!
                        90                     JBC C,SUM_LOOP_3
                        91
                        92                     JoaNhBinZuUnkreativ1:
                        93                             xch A,R2                ;tausche A mit R2
                        94                             ORL A, R7               ;schreibe wert in TicTacToeFeld mit |-Verknüpfung
                        95                             MOV R7, A               ;speichere Wert in Register zwischen
                        96                             JMP MAIN_LOOP
                        97                             ;RET                    ;RET, wenn Interrupt verwendet wird
                        98
                        99             SUM_LOOP_3:
                       100                     xch A,R2
                       101                     INC R2                  ; Inkrementiere den Zählerwert für die nächste Iteration
                       102                     RL A            ;rotiere akku um 3 Stellen je übertrag
                       103                     RL A
                       104                     RL A
                       105                     xch A,R2                ;tausche A(kku) mit R2
                       106                     JNZ SUM_LOOP_3          ; springe zurück zur Schleife, wenn r2 != 0
                       107                     JZ JoaNhBinZuUnkreativ1
                       108
                       109             ;-----------------------------------------------------------------------
                       110             ; 3 < x < 6
                       111             LESS_THAN_6:
                       112                     MOV R2, A
                       113                     MOV A, #00000011b
                       114                     xch A,R2
                       115                     JBC C,SUM_LOOP_6
                       116
                       117                     JoaNhBinZuUnkreativ2:
                       118                             xch A,R2
                       119                             ORL A, R6
                       120                             MOV R6, A
                       121                             JMP MAIN_LOOP
                       122                             ;RET            RET, wenn Interrupt verwendet wird
                       123
                       124             SUM_LOOP_6:
                       125                     xch A,R2
                       126                     INC R2
                       127                     RL A
                       128                     RL A
                       129                     RL A
                       130                     xch A,R2
                       131                     JNZ SUM_LOOP_6
                       132                     JZ JoaNhBinZuUnkreativ2
                       133
                       134
                       135
                       136             ;-----------------------------------------------------------------------
                       137             ; 6< x < 9
                       138             LESS_THAN_9:
                       139                     MOV R2, A
                       140                     MOV A, #00000011b
                       141                     xch A,R2
                       142                     JBC C,SUM_LOOP_9
                       143
                       144                     JoaNhBinZuUnkreativ3:
                       145                             xch A,R2
                       146                             ORL A, R5
                       147                             MOV R5, A
                       148                             JMP MAIN_LOOP
                       149                             ;RET
                       150
                       151             SUM_LOOP_9:
                       152                     xch A,R2
                       153                     INC R2
                       154                     RL A
                       155                     RL A
                       156                     RL A
                       157                     xch A,R2
                       158                     JNZ SUM_LOOP_9
                       159                     JZ JoaNhBinZuUnkreativ3
                       160
                       161
                       162
                       163
                       164
                       165     DISPLAY_BOARD:
                       166             ;-----------------------------------------------------------------------
                       167                 ; Zeige R7,R6,R5 nacheinander gemultiplext an
                       168                 ; R7, R6, R5 spiegeln das Feld wider, x die Eingabe aus R0 von Funktion ON_INPUT (Wird au
                       169             ;-----------------------------------------------------------------------
                       170
                       171
                       172
                       173             MOV P1, #00000000b ;setze p1 standardgemäß auf 0
                       174             ;display0:
                       175                     MOV P2, R7
                       176                     setb P1.0
                       177                     JBC P1.0,display1
                       178             display1:
                       179                     ;MOV P2, R7
                       180                     setb P1.1
                       181                     JBC P1.1,display2
                       182             display2:
                       183                     ;MOV P2, #00000000b
                       184                     setb P1.2
                       185                     JBC P1.2,display3
                       186             display3: MOV P2, R6
                       187                     setb P1.3
                       188                     JBC P1.3,display4
                       189             display4:
                       190                     setb P1.4
                       191                     JBC P1.4,display5
                       192             display5:
                       193                     ;MOV P2, #00000000b
                       194                     setb P1.5
                       195                     JBC P1.5,display6
                       196             display6:
                       197                     MOV P2, R5
                       198                     setb P1.6
                       199                     JBC P1.6,display7
                       200             display7:
                       201                     setb P1.7
                       202
                       203
                       204                     call main_loop
