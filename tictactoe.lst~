                         9     ;-----------------------------------------------------------------------
                        10     ; Voreingestellungen
                        11     ;-----------------------------------------------------------------------
                        12     ORG 0H   ; Startadresse des Programms
                        13     MOV P1, #00H    ; Konfiguriere Port 1 als Ausgangsport für LED-Matrix
                        14     MOV P2, #00H    ; Konfiguriere Port 2 als Ausgangsport für LED-Matrix
                        15
                        16     ;Bits für Interrupt
                        17     SETB EA
                        18     SETB IE0        ; Setzte Bit IE1 für Interruptfreigabe 3.2
                        19     SETB EX0        ; Setzte Bit EX0 für Interruptfreigabe (Port 3.2)
                        20     SETB PX0        ; Setzte Priorität für Interruptfreigabe (Port 3.2)
                        21
                        22
                        23     call INIT_BOARD
                        24     call MAIN_LOOP
                        25     ;-----------------------------------------------------------------------
                        26     ;ISR
                        27     ;-----------------------------------------------------------------------
                        28     ORG 13H ; adresse von interrupt 3.2
                        29     call ON_INPUT
****ERROR:Unabletooverwritealreadyreservedprogrammemoryataddress0x13--compilationfailed
                        30     reti ; return from interupt; remove interrupt-bit
